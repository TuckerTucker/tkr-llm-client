{
  "name": "@tkr/llm-client",
  "version": "0.1.0",
  "description": "Generic LLM client library with Claude SDK and local LLM server support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TuckerTucker/tkr-llm-client.git"
  },
  "bugs": {
    "url": "https://github.com/TuckerTucker/tkr-llm-client/issues"
  },
  "homepage": "https://github.com/TuckerTucker/tkr-llm-client#readme",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "verify-exports": "npx tsx scripts/verify-exports.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:live": "vitest run tests/live",
    "prepublishOnly": "npm run rebuild && npm run verify-exports"
  },
  "keywords": [
    "llm",
    "claude",
    "ai",
    "machine-learning",
    "mlx",
    "local-llm",
    "typescript",
    "fastapi",
    "anthropic"
  ],
  "author": "Tucker",
  "license": "Apache-2.0",
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.36",
    "@dagrejs/dagre": "^1.1.8",
    "d3-force": "^3.0.0",
    "dagre": "^0.8.5",
    "elkjs": "^0.11.0",
    "framer-motion": "^12.23.24",
    "js-yaml": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reactflow": "^11.10.0",
    "zustand": "^4.4.0"
  },
  "peerDependencies": {
    "mlx-lm": "^0.8.0"
  },
  "peerDependenciesMeta": {
    "mlx-lm": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/d3-force": "^3.0.10",
    "@types/dagre": "^0.7.53",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitest/coverage-v8": "^4.0.8",
    "@vitest/ui": "^4.0.8",
    "typescript": "^5.3.0",
    "vitest": "^4.0.8"
  },
  "files": [
    "dist",
    "llm_server",
    "README.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
